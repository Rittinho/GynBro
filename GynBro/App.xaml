<?xml version="1.0" encoding="UTF-8" ?>
<Application
    x:Class="GynBro.App"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:GynBro">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Styles/Colors.xaml" />
                <ResourceDictionary Source="Resources/Styles/Styles.xaml" />

                <ResourceDictionary Source="Resources/Styles/Global/Buttons.xaml" />
                <ResourceDictionary Source="Resources/Styles/Global/TabBar.xaml" />
                <ResourceDictionary Source="Resources/Styles/Global/Container.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
